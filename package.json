{
  "name": "actfast",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "EXPO_USE_FAST_RESOLVER=1 bunx expo start -c",
    "android": "npx expo prebuild && bun expo run:android",
    "build:android": "npx expo prebuild",
    "ios": "bun expo run:ios",
    "ios:prebuild": "npx expo prebuild && bun expo run:ios",
    "web": "bun run expo start --web",
    "test": "bun run jest --watchAll",
    "format": "bun run prettier --write .",
    "format:check": "bun run prettier --check .",
    "clean": "rm -rf node_modules; rm -rf ios/build; rm -rf ios/build; rm -rf ios/Pods; rm -rf ios/Podfile.lock; rm -rf ~/Library/Developer/Xcode/DerivedData/",
    "lint": "eslint .",
    "eas:prep": "npx expo-doctor && npx expo prebuild",
    "eas:deploy": "eas build --platform android",
    "eas:dev": "eas build --platform android --profile development"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "cactus-react-native": "^0.2.9",
    "expo": "53.0.22",
    "expo-av": "^16.0.7",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-media-library": "^17.1.7",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-device-info": "^14.0.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-mmkv": "2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.0.10",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "globals": "^15.8.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
